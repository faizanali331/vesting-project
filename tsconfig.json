{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "strictPropertyInitialization": false,
    "types": [
      "node",
      "hardhat"
    ]
  },
  "include": [
    "scripts",
    "test",
    "typechain",
    "hardhat.config.ts",
    "server.ts",
    "listner.ts",
    "src/indexer.ts",
    "src/server.ts"
  ]
}